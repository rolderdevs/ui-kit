{
  "name": "@rolder/ui-kit-react",
  "description": "Rolder UI Kit for React",
  "version": "3.0.0",
  "type": "module",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "types": "./dist/index.ts",
    "import": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "bump": "echo '\n=== Bump UI-Kit React ===' && bunx bumpp --no-tag",
    "release": "bun run bump && bun scripts/publish.js"
  },
  "devDependencies": {
    "@rolder/ss-react": "workspace:*",
    "@pandacss/dev": "^1.4.0",
    "@rsbuild/core": "~1.5.13",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.14.0",
    "@types/react": "^19.1.15"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
