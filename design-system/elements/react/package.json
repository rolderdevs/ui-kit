{
	"name": "@rolder/ui-kit-react",
	"version": "3.2.0",
	"type": "module",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		"types": "./src/index.ts",
		"import": "./src/index.ts"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rslib build && bun run build:panda-ship",
		"build:panda-ship": "bun panda ship --outfile dist/panda.buildinfo.json",
		"dev": "rslib build --watch",
		"bump": "echo '\n=== Bump UI-Kit React ===' && bunx bumpp --no-tag",
		"release": "bun run bump && bun scripts/publish.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@rolder/ss-react": "workspaces:*",
		"@pandacss/dev": "^1.4.0",
		"@rsbuild/core": "~1.5.12",
		"@rsbuild/plugin-react": "^1.4.1",
		"@rslib/core": "^0.14.0",
		"@types/react": "^19.1.13"
	},
	"peerDependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	}
}
